<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a365d;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d3748;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#bg)" />
  
  <!-- Mathematical Elements -->
  <g fill="none" stroke="#60a5fa" stroke-width="2" opacity="0.6">
    <!-- Basic Geometry Shapes -->
    <circle cx="200" cy="300" r="50" />
    <rect x="300" y="250" width="100" height="100" />
    <polygon points="500,250 550,350 450,350" />
    
    <!-- Mathematical Symbols -->
    <text x="150" y="200" fill="#f0f9ff" font-family="Arial" font-size="24">+</text>
    <text x="250" y="200" fill="#f0f9ff" font-family="Arial" font-size="24">−</text>
    <text x="350" y="200" fill="#f0f9ff" font-family="Arial" font-size="24">×</text>
    <text x="450" y="200" fill="#f0f9ff" font-family="Arial" font-size="24">÷</text>
    
    <!-- Number Line -->
    <line x1="100" y1="400" x2="700" y2="400" stroke="#60a5fa" />
    <g fill="#f0f9ff" font-family="Arial" font-size="16">
      <text x="100" y="430">0</text>
      <text x="200" y="430">1</text>
      <text x="300" y="430">2</text>
      <text x="400" y="430">3</text>
      <text x="500" y="430">4</text>
      <text x="600" y="430">5</text>
      <text x="700" y="430">6</text>
    </g>
  </g>
  
  <!-- Decorative Elements -->
  <g opacity="0.2">
    <circle cx="100" cy="100" r="20" fill="#60a5fa" />
    <circle cx="700" cy="500" r="30" fill="#60a5fa" />
    <circle cx="400" cy="150" r="15" fill="#60a5fa" />
  </g>
</svg>