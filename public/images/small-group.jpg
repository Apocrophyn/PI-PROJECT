<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#312e81;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4c1d95;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#bg)" />
  
  <!-- Round Table -->
  <circle cx="400" cy="350" r="150" fill="#475569" />
  
  <!-- Students Around Table -->
  <!-- Student 1 (Left) -->
  <g transform="translate(250,350)">
    <circle cx="0" cy="0" r="25" fill="#94a3b8" />
    <path d="M -8 30 L 8 30 L 0 70 Z" fill="#94a3b8" />
    <rect x="-30" y="40" width="60" height="40" fill="#e2e8f0" />
  </g>
  
  <!-- Student 2 (Right) -->
  <g transform="translate(550,350)">
    <circle cx="0" cy="0" r="25" fill="#94a3b8" />
    <path d="M -8 30 L 8 30 L 0 70 Z" fill="#94a3b8" />
    <rect x="-30" y="40" width="60" height="40" fill="#e2e8f0" />
  </g>
  
  <!-- Student 3 (Top) -->
  <g transform="translate(400,200)">
    <circle cx="0" cy="0" r="25" fill="#94a3b8" />
    <path d="M -8 30 L 8 30 L 0 70 Z" fill="#94a3b8" />
    <rect x="-30" y="40" width="60" height="40" fill="#e2e8f0" />
  </g>
  
  <!-- Tutor -->
  <g transform="translate(400,500)">
    <circle cx="0" cy="0" r="30" fill="#60a5fa" />
    <path d="M -10 40 L 10 40 L 0 80 Z" fill="#60a5fa" />
  </g>
  
  <!-- Shared Learning Materials -->
  <g transform="translate(400,350)">
    <!-- Central Study Material -->
    <rect x="-50" y="-30" width="100" height="60" fill="#e2e8f0" />
    <line x1="-40" y1="-10" x2="40" y2="-10" stroke="#64748b" stroke-width="2" />
    <line x1="-40" y1="10" x2="40" y2="10" stroke="#64748b" stroke-width="2" />
  </g>
  
  <!-- Interaction Lines -->
  <g stroke="#818cf8" stroke-width="2" opacity="0.6">
    <path d="M 280 350 C 320 320 380 320 400 350" fill="none" />
    <path d="M 520 350 C 480 320 420 320 400 350" fill="none" />
    <path d="M 400 240 C 400 280 400 320 400 350" fill="none" />
  </g>
  
  <!-- Mathematical Elements -->
  <g fill="#f0f9ff" font-family="Arial" font-size="20" opacity="0.8">
    <text x="200" y="150">∑</text>
    <text x="500" y="150">π</text>
    <text x="350" y="100">√</text>
  </g>
  
  <!-- Decorative Elements -->
  <g opacity="0.2">
    <circle cx="100" cy="100" r="20" fill="#818cf8" />
    <circle cx="700" cy="500" r="30" fill="#818cf8" />
    <circle cx="400" cy="50" r="15" fill="#818cf8" />
  </g>
</svg>